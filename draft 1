void euler(int nt, double t[nt], double x0, double x[nt]){     //xi+1
    int i;
    double dt;
    double y[0] = 0;
    for(i=0;i<nt-1;i++){
        dt = t[i+1]-t[i];
        x[i+1] = x[i] + dt*f(x[i],t[i]);
    }
}

void euler(int nt, double t[nt], double y0, double y[nt]){  //x'i+1
    int i;
    double dt;
    y[0] = y0;
    for(i=0;i<nt-1;i++){
        dt = t[i+1]-t[i];
        yp[i+1] = yp[i] + dt*f(y[i],t[i]);
    }
}

